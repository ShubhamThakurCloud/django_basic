>>> user
<User: shubhamTK>
>>> post_2.save()
>>> user.post_set
<django.db.models.fields.related_descriptors.create_reverse_many_to_one_manager.<locals>.RelatedManager object at 0x7f31796ed9f0>
>>> user.post_set.all()
<QuerySet [<post: Blog 1>, <post: Blog 2>]>
>>> user.post_set.create(title='Blog 3' , content='Third Post content!')
<post: Blog 3>
>>> user.post_set
<django.db.models.fields.related_descriptors.create_reverse_many_to_one_manager.<locals>.RelatedManager object at 0x7f31796ed9f0>
>>> user.post_set.all()
<QuerySet [<post: Blog 1>, <post: Blog 2>, <post: Blog 3>]>
>>> exit()

